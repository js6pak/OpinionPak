<Project>
    <PropertyGroup>
        <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>

        <!-- https://github.com/dotnet/sdk/blob/v8.0.200/eng/Versions.props#L95 -->
        <NuGetPackageVersion>6.9.1</NuGetPackageVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageVersion Include="BepInEx.AssemblyPublicizer.MSBuild" Version="0.4.2" />

        <PackageVersion Include="NuGet.Versioning" Version="$(NuGetPackageVersion)" />
        <PackageVersion Include="NuGet.ProjectModel" Version="$(NuGetPackageVersion)" />

        <PackageVersion Include="Microsoft.NET.Test.Sdk" Version="17.9.0" />
        <PackageVersion Include="xunit" Version="2.7.0" />
        <PackageVersion Include="xunit.runner.visualstudio" Version="2.5.7" />
        <PackageVersion Include="coverlet.collector" Version="6.0.1" />
        <PackageVersion Include="GitHubActionsTestLogger" Version="2.3.3" />

        <PackageVersion Include="AssemblyShader" Version="1.0.3-preview" />
        <PackageVersion Include="MSBuild.ProjectCreation" Version="11.0.1" />
        <PackageVersion Include="Microsoft.TemplateEngine.Authoring.Tasks" Version="8.0.102" />
        <PackageVersion Include="SimpleExec" Version="12.0.0" />
    </ItemGroup>
</Project>
